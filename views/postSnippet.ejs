<div class="post">

    <div class="post-heading">
        <div class="post-title">
            <h3><a href=<%= post.url %>><%= post.title %></a></h3>
        </div>
        <span class="date">
        <%= (post.date.getMonth() + 1) + '.' + post.date.getDate() + '.' + post.date.getFullYear() %>
      </span>
    </div>

    <div class="post content">
        <%- post.preview %>
    </div>

</div>
